<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>형상관리_고도화</display-name>
  <welcome-file-list>
    <!-- 
    <welcome-file>login_sso.jsp</welcome-file>
    -->
    <welcome-file>/webPage/login/ecamsLogin.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>400</error-code>
    <location>/webPage/error/error_400.jsp</location>
  </error-page>
  <error-page>
    <error-code>401</error-code>
    <location>/webPage/error/error_401.jsp</location>
  </error-page>
  <error-page>
    <error-code>403</error-code>
    <location>/webPage/error/error_403.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/webPage/error/error_404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/webPage/error/error_500.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/webPage/error/error.jsp</location>
  </error-page>
